package arraydemo;

public class Example2 {
	
	public static int[] getSum(int arr[],int sum) {
		for(int i=0;i<arr.length;i++) {
			i+=sum;
			if(sum == arr[i]) {
				
				System.out.println(arr[i]);
				
			}
			}
		return null;
	}
	
	public static void main(String[] args) {
		
		int[] arr= {6,3,7,10};
		System.out.println(Example2.getSum(arr, 4));
		
	}

}
