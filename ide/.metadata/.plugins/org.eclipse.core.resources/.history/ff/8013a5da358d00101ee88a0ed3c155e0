package arraydemo;

public class Example3 {
	
	public int sumOfArray(int[] arr) {
		for(int i=0;i<arr.length;i++) {
			System.out.println(i+arr[i]);
			
		}
		return 0;
	}
	
	public static void main(String[] args) {
		int [] arr= {1,6,4,3,2,9};
		Example3 b=new Example3();
		System.out.println(b.sumOfArray(arr));
	}

}
