package pkg1;

public class Prime {
	public static void main(String[] args) {
		int i=2;
		int num=17;
		boolean isPrime=true;
		while(i<=num/2) {
			if(num%i==0) {
				isPrime=false;
				System.out.println("Not a prime number");
			}else {
				
				System.out.println("prime number");
				break;
			}
		}
		
	}
	

}
