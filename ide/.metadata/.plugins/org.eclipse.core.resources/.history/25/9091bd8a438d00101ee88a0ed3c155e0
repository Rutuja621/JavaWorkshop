package arraydemo;

public class Example5 {
	
	public static int [] getSum(int [] a) {
		int sum=0;
		for(int i=0;i<a.length;i++) {
			for(int j=i+1;j<a.length;j++) {
				sum=i+j;
				System.out.println(sum);
			}
		}
		return null;
	}
	
	public static void main(String[] args) {
		int [] a= {0,1,2,3,4,5};
		
        int[] result = Example5.getSum(a);
        
	}

}
