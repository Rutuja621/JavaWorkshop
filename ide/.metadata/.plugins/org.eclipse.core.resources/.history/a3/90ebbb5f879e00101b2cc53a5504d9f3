		System.out.println("Enter rollNo: ");
		int rollNo=sc.nextInt();
		System.out.println("Enter City: ");
		int city=sc.nextInt();
		
		Employee st=new Employee(rollNo ,name,city);
		
		System.out.println(st);
		Employee.add(st);
		
		System.out.println("Press 1 for add more");
		if(1!=sc.nextInt()) {
			addMore=false;
		}
		
		public void getAllEmployee() {
			Iterator<Employee> itr=employee.iterator();
			while(itr.hasNext()) {
				Employee emp=oit.next();
				System.out.println(emp.getCity());
