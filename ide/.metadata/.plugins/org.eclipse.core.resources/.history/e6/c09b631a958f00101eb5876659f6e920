package wrapper.com;

public class WrapperDemo {
	public static void main(String[] args) {
		int a=10;
		//here conversion of int to Interger  and called as auto boxing
		//primitive to wrapper coversion is calles autoboxing
		Integer ab=10;
		      //  wrapper
/*		int --->Integer
 *      float --->Float
 *      boolean --->Boolean
 *      char ---->Character
 * 
 */
		
		Integer e=100;
		int f=e;//conversion of wrapper class to primitive called unboxing
		
		
		
		int abc=1002;//primitive
		String num=Integer.toString(abc);//conversion of primitive to wrapper and then to string
		String opq=String.valueOf(abc);//conversion of primitive to string
		
		System.out.println(num);
		
		
		//string to primitive
		String num1="12345";
		int num2=Integer.parseInt(num1);//parseInt is an static type function
		//two types of function static and nonstatic
		System.out.println(num2);
		
		//string to integer not possible
		/*String name="Rutuja";
		int num3=Integer.parseInt(name);
		System.out.println(num3);*/
	}

}
