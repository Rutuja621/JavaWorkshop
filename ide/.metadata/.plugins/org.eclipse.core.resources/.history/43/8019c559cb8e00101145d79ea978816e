package StringDeclare;

public class Replacestr {
	//return the output as : bcdefa
	public static String changeSequence(String a) {
		for(int i=0;i<a.length();i++) {
			return a.substring(1)+a.charAt(0);
		}
		return a;
	}
	
	//return output as  -- 'fl'
	public static String longestString(String [] arr) {
		
		return arr.toString();
	}
	

	
	public static void main(String[] args) {
		String a="abcdef";
		System.out.println(Replacestr.changeSequence(a));
		
		String[] ab= {"flower","fly","flow","flight"};
		System.out.println(Replacestr.longestString(ab));
	}

}
