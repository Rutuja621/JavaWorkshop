package pkg1;

public class Even50no {
	public static void main(String[] args) {
		for(int i=0;i<=50;i++) {
			System.out.println(i);
//			if(i%2==0) {
//			System.out.println(i);
			if(i==3) {
				break;
			}
//			}
		}
		
		//continue
		System.out.println("continue");
		for(int i=0;i<6;i++) {
			if(i==3) {
				continue;
				
			}
			System.out.println(i);
		}
	}

}
