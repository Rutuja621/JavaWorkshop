package arraydemo;

public class Example4 {
	
	public static int[] getAbridgedVersion(int a[]) {
		return a;
	}
	
	
	public static void main(String[] args) {
		int a[]= {10,2,3,2,40,40,30};
		
		//output 10,2,3,40,30
		
		int [] version=Example4.getAbridgedVersion(a);
		
		for(int d:version) {
			System.out.println(version);
		}
	}

}
